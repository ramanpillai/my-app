node{
    stage('SCM Checkout'){
      git 'https://github.com/javahometech/my-app'
      }
    
    stage('Compile-Package'){
    //Get MVN home directory
    def mvnHome = tool name: 'MavenLocal', type: 'maven'
    sh "${mvnHome)/bin/mvn package"
   //  sh '/opt/apache-maven-3.6.3/bin/mvn package'
      }
 }
